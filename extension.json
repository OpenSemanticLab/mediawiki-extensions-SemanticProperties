{
    "manifest_version":2,
    "name":"SemanticProperties",
    "version":"0.0.1",
    "author":[
        "[https://github.com/simontaurus Simon Stier]"
    ],
    "url":"https://github.com/simontaurus/mediawiki-extensions-SemanticProperties",
    "descriptionmsg":"semantic-properties-desc",
    "namemsg":"semantic-properties-name",
    "license-name":"AGPL-3.0-or-later",
    "type":"editor",
    "requires":{
        "MediaWiki":">= 1.31"
    },
    "MessagesDirs":{
        "SemanticProperties":[
            "i18n"
        ]
    },
    "ResourceModules":{
        "ext.SemanticProperties":{
            "localBasePath":"modules/ext.SemanticProperties",
            "remoteExtPath":"SemanticProperties/modules/ext.SemanticProperties",
            "styles":[
                "SemanticProperties.css"
            ],
            "scripts":[
                "SemanticProperties.js"
            ],
            "position":"bottom"
        }
    },
    "AutoloadClasses":{
        "SemanticProperties":"includes/SemanticProperties.php"
    },
    "Hooks":{
        "BeforePageDisplay":"SemanticProperties::onBeforePageDisplay",
        "PageImporterRegisterPageLists": "SemanticProperties::onPageImporterRegisterPageLists",
    }
}
